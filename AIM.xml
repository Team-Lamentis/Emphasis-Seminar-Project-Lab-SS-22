<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
chan car_move, car_wai, carcom  ,cararrive ,carend ,carrea;</declaration>
	<template>
		<name x="5" y="5">Car</name>
		<declaration>// Place local declarations here.
clock wait;</declaration>
		<location id="id0" x="25" y="170">
		</location>
		<location id="id1" x="314" y="170">
			<name x="238" y="195">car_communicate</name>
		</location>
		<location id="id2" x="187" y="170">
			<name x="136" y="195">car_arrive</name>
		</location>
		<location id="id3" x="510" y="42">
			<name x="476" y="8">car_move</name>
		</location>
		<location id="id4" x="535" y="280">
			<name x="501" y="297">car_wait</name>
		</location>
		<location id="id5" x="739" y="161">
			<name x="731" y="178">end</name>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id4"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="522" y="144">carrea!</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="528" y="84">carend!</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id4"/>
			<label kind="guard" x="382" y="238">move == 0</label>
			<label kind="synchronisation" x="382" y="255">car_wait!</label>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id3"/>
			<label kind="guard" x="357" y="59">move == 1</label>
			<label kind="synchronisation" x="357" y="76">car_move!</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id1"/>
			<label kind="synchronisation" x="221" y="144">carcom!</label>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="synchronisation" x="68" y="144">cararrive!</label>
		</transition>
	</template>
	<system>// Place template instantiations here.
carone = Car();
cartwo =Car();
// List one or more processes to be composed into a system.
system carone,cartwo;
 </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
